#include <iostream>
using namespace std;
int main(){

    int x,y;
    cout<<"Enter x coordinate: ";
    cin>>x;
    cout<<"Enter y Coordinate: ";
    cin>>y;

    if(x>0 && y>0){
        cout<<"The point is in the 1st Quadrant";    }
    
    else if(x<0 && y>0){
        cout<<"The point is in the 2nd Quadrant";}

    else if(x<0 && y<0){
        cout<<"The point is in the 3rd Quadrant";}
        
    else if(x>0 && y<0){
        cout<<"The point is in 4th Quadrant";}

    else if (x==0 && y!=0)
    cout<<"The point is on the Y-axis";
    
    else if (x!=0 && y==0)
    cout<<"The point is on the X-axis";
    
    else
    cout<<"The point is at Origin";
    


}


/* OUTPUT
Enter x coordinate: -6
Enter y Coordinate: 1
The point is in the 2nd Quadrant

=== Code Execution Successful ===*/
