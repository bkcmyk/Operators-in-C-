#include <iostream>
using namespace std;

int main()
{
    int maths, chemistry, physics, english, biology;
    cout << "Enter Maths score: ";
    cin >> maths;

    cout << "Enter Chemistry score: ";
    cin >> chemistry;

    cout << "Enter Physics score: ";
    cin >> physics;

    cout << "Enter English score: ";
    cin >> english;

    cout << "Enter Biology score: ";
    cin >> biology;

    int average = (maths + chemistry + physics + english + biology) / 5;

    cout << "Average Score: " << average << endl;

    if (average >= 90)
        cout << "Grade: O" << endl;
    else if (average >= 80)
        cout << "Grade: A" << endl;
    else if (average >= 70)
        cout << "Grade: B" << endl;
    else if (average >= 60)
        cout << "Grade: C" << endl;
    else if (average >= 50)
        cout << "Grade: D" << endl;
    else
        cout << "Grade: FAIL" << endl;

    return 0;
}


/* OUTPUT
Enter Maths score: 75
Enter Chemistry score: 85
Enter Physics score: 52
Enter English score: 90
Enter Biology score: 96
Average Score: 79
Grade: B


=== Code Execution Successful === */
