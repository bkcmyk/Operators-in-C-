#include <iostream>
using namespace std;

int main() {
    int a;
    cout << "Enter an integer: ";
    cin >> a;

    if (a > 0) {
        cout << "The number is positive." << endl;
    }
    else if (a < 0) {
        cout << "The number is negative." << endl;
    }
    else {
        cout << "The number is equal to 0." << endl;
    }

    return 0;
}

/* OUTPUT

Enter an integer: 75
The number is positive. 

*/
